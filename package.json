{
	"name": "chatguessr",
	"version": "1.0.11",
	"description": "Chatbot for Geoguessr",
	"author": "MrAmericanMike & T.ZHF",
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "https://github.com/tzhf/chatguessr.git"
	},
	"main": "./src/main.js",
	"dependencies": {
		"axios": "^0.22.0",
		"codegrid-js": "git+https://github.com/Tzhf/codegrid-js.git",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"electron-store": "^8.0.1",
		"electron-updater": "^4.3.9",
		"express": "^4.17.1",
		"jquery": "^3.5.1",
		"tmi.js": "^1.8.5"
	},
	"devDependencies": {
		"electron": "^15.1.1",
		"electron-builder": "^22.11.7"
	},
	"scripts": {
		"start": "electron .",
		"pack": "electron-builder --dir",
		"dist": "electron-builder",
		"postinstall": "electron-builder install-app-deps",
		"build": "electron-builder build --win --publish never",
		"deploy": "electron-builder build --win --publish always"
	},
	"build": {
		"appId": "com.tzhf.chatguessr",
		"productName": "Chatguessr",
		"win": {
			"target": "nsis",
			"icon": "build/icon.ico"
		},
		"nsis": {
			"deleteAppDataOnUninstall": true
		}
	}
}
